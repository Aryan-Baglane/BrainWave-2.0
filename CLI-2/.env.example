# OpenRouter API Configuration
# Get your key from: https://openrouter.ai/keys
OPENROUTER_API_KEY=sk-or-v1-your-key-here
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# Model Selection
LLM_MODEL=openai/gpt-3.5-turbo

# Optional: Logging Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Optional: Temperature for LLM (0.0 - 2.0)
LLM_TEMPERATURE=0.1

# Optional: Max Tokens
LLM_MAX_TOKENS=2000

# Optional: Your app name for OpenRouter
APP_NAME=AI-Data-Engineering-System

# On-Demand API Configuration
# Get your key and workflow IDs from your On-Demand account
ONDEMAND_API_KEY=your_ondemand_api_key_here
ONDEMAND_BASE_URL=https://api.on-demand.io/automation/api/workflow

# Workflow IDs for each On-Demand Agent
ONDEMAND_ARCHITECTURE_WORKFLOW=your_workflow_id_here
ONDEMAND_VISUALIZER_WORKFLOW=your_workflow_id_here
ONDEMAND_SUMMARIZER_WORKFLOW=your_workflow_id_here
ONDEMAND_JARGON_TRANSLATOR_WORKFLOW=your_workflow_id_here
ONDEMAND_REVERSIBILITY_WORKFLOW=your_workflow_id_here
ONDEMAND_TOP_COLUMNS_WORKFLOW=your_workflow_id_here

# Setup Instructions:
# 1. Copy this file to .env
# 2. Replace placeholder values with your actual API keys
# 3. Never commit .env to version control
# 4. Keep .env file in .gitignore# Optional: Temperature for LLM (0.0 - 2.0)
LLM_TEMPERATURE=0.1

# Optional: Max Tokens
LLM_MAX_TOKENS=2000

# Optional: Your app name for OpenRouter
APP_NAME=AI-Data-Engineering-System
